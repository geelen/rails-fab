{
  "name": "rails_fab",
  "private": true,
  "dependencies": {
    "@rails/actioncable": "^6.0.0-alpha",
    "@rails/activestorage": "^6.0.0-alpha",
    "@rails/ujs": "^6.0.0-alpha",
    "@rails/webpacker": "^4.0.7",
    "turbolinks": "^5.2.0"
  },
  "scripts": {
    "build": "bin/webpack",
    "build:fab": "FAB_BUILD=true npm run build && npm run fab:compile",
    "fab:clean": "rm -rf .fab",
    "fab:prepare": "mkdir -p .fab/intermediate/_server && cp -R dynamic/fab-server/* .fab/intermediate/_server && cp -R app/assets/javascripts/React .fab/intermediate",
    "fab:compile": "npm run fab:clean && npm run fab:prepare && fab-compile"
  },
  "version": "0.1.0"
}
